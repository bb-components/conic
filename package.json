{
  "name": "conic",
  "version": "1.2.1",
  "description": "website provider",
  "main": "dist/index.js",
  "scripts": {
    "server": "node server/index.js",
    "webpack-dev": "node webpack.js",
    "webpack-dist": "webpack -p --profile --display-modules",
    "cdn": "webpack -p --profile --config webpack.config.cdn.js",
    "start": "npm run cdn && npm run server & npm run webpack-dev",
    "build": "npm run cdn && npm run webpack-dist && npm run server",
    "render": "babel server/render.js",
    "push": "git add -A && git commit -m \"quick push\" && git push",
    "pub": "webpack --config webpack.config.publish.js && ./publish.sh"
  },
